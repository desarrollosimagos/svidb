function buscar(id){
	document.buscarid.action="/panel/coord/index/"+ {{ id2 }};
 	window.location.href="/panel/coord/index/buscar/"+ {{ id2 }} + "/" + id;
}
function urlForm(id){
	document.idBuscarSuperior.action="/panel/coord/index/buscar/"+ {{ id2 }} + "/" + id;
	}
